{
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.shell.osx": "/bin/zsh",

  "workbench.iconTheme": "material-icon-theme",
  "debug.console.fontFamily": "Fira Code Retina",

  "window.zoomLevel": 0,
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.lineHeight": 24,
  "editor.formatOnSave": true,
  "update.mode": "none",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120],
  "editor.tabSize": 2,
  "terminal.integrated.fontSize": 14,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d"
  },
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "editor.formatOnSave": true,
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "desktop": "dump"
  },

  "breadcrumbs.enabled": true,
  "editor.parameterHints.enabled": true,
  "typescript.tsserver.log": "verbose",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "prettier.singleQuote": true,
  //Font-family
  "editor.fontFamily": "'Fire Code Retina', Consolas, 'Courier New', monospace",
  "java.home": "C:\\Users\\jotal\\AppData\\Local\\Programs\\AdoptOpenJDK",
  "java.help.firstView": "gettingStarted",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.configuration.checkProjectSettingsExclusions": false,
  "json.schemas": [
    {
      "name": "vscode-expo-manifest",
      "url": "file:///c%3A/Users/jotal/AppData/Roaming/Code/User/globalStorage/bycedric.vscode-expo/manifest-38.0.0.json",
      "fileMatch": ["app.json", "app.config.json"]
    }
  ],
  "git.ignoreMissingGitWarning": true,
  "workbench.colorTheme": "Halcyon"
}
